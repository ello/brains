    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "eslint": "3.19.0",
    "eslint-config-ello": "1.0.3",
    "flow-bin": "0.45.0"
  },
  "dependencies": {
    "humps": "2.0.0",
    "immutable": "3.8.1",
    "lodash": "4.17.4",
    "react-router-redux": "4.0.8",
